package com.ithup.demo;
/**
 * 线程模拟购票火车票
 * 	实现Runnable接口的方式实现共享
 * @author acer
 *
 */
class Ticker implements Runnable{
	int ticker=5;
	@Override
	public void run() {
		for(int i=0;i<ticker;i++){
			System.out.println("购票："+i);
		}
	}
}
public class ThreadTicker {
	public static void main(String[] args) {
		Ticker t = new Ticker();
	}
}
