package com.iuthuplonl.io;

import java.io.FileInputStream;

/**
 * 应用FileInputStream读取文件
 * @author acer
 *
 */
public class FileDemo03 {
	public static void main(String[] args) {
		FileInputStream fis = null;
		//创建文件流对象
		try {
			fis = FileInputStream("D:\\javacode\\file\\a.txt");
		} catch (Exception e) {
			e.printStackTrace();
		}
	}
}
