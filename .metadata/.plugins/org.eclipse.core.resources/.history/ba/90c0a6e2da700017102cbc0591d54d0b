package com.iuthuplonl.io;

import java.io.File;
import java.io.IOException;

public class FileMethod {
	public static void main(String[] args) {
		
	}
class FileMethod03{
	/**
	 * 创建文件
	 * @throws IOException 
	 */
	public void createFile(File file){
		//判断文件是否存在
		if(!file.exists()){
			try {
				file.createNewFile();		//创建新的文件
			} catch (IOException e) {
				e.printStackTrace();
			}	
		}
	}
}
}
