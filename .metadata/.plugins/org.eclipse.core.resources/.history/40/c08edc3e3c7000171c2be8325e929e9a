package day14;

import java.util.HashMap;
import java.util.Map;

/**
 * 通过Map,显示对应的键值信息
 * @author acer
 *
 */


public class MapTest2 {
	//程序的入口
	public static void main(String[] args) {
		//创建map集合
		Map<String,String> pet = new HashMap<>();
		//添加数据
		pet.put("欧欧", "ouou");
		pet.put("幽幽", "youyou");
		pet.put("陌陌", "momo");
		pet.put("嘎嘎", "gaga");
		pet.put("娜娜", "nana");
		//取出欧欧对应的值
		String ouou = (String) pet.get("欧欧");
		System.out.println("CN代表的是："+ouou);
		//输出map中的数量
		System.out.println("map中共有："+countries.size()+"条记录");
		//map是否包含FR的key
		System.out.println("map中是否包含FR的key "+countries.containsKey("FR"));
		//删除map中的FR
		countries.remove("FR");
		//map是否包含FR的key
		System.out.println("map中是否包含FR的key "+countries.containsKey("FR"));
		//输出所有key信息
		System.out.println(countries.keySet());
		//输出所有value信息
		System.out.println(countries.values());
		//输出所有key:values
		System.out.println(countries);
	}
}
