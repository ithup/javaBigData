package day14;
/**
 * 宠物类：狗和企鹅
 * @author acer
 *
 */
public class Pet {
	String name;	//昵称
	int health;		//健康值
	int love;		//亲密度
	/**
	 * 构造方法：父类
	 * @param name
	 */
	public Pet(String name){
		this.name=name;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getHealth() {
		return health;
	}
	public void setHealth(int health) {
		this.health = health;
	}
	public int getLove() {
		return love;
	}
	public void setLove(int love) {
		this.love = love;
	}
	
}
