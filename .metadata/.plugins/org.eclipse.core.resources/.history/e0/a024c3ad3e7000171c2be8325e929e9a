package day14;

import java.util.HashMap;
import java.util.Map;

/**
 * 通过Map,显示对应的键值信息
 * 
 * @author acer
 *
 */

public class MapTest3 {
	// 程序的入口
	public static void main(String[] args) {
		// 创建map集合
		Map<String, String> pet = new HashMap<>();
		// 添加数据
		pet.put("欧欧", "ouou");
		pet.put("幽幽", "youyou");
		pet.put("陌陌", "momo");
		pet.put("嘎嘎", "gaga");
		pet.put("娜娜", "nana");
		
	}
}
