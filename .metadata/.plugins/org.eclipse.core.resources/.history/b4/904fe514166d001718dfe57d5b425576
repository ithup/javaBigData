package day12;

/**
 * 局部内部类
 * 
 * @author acer
 *
 */
class Outer2 {
	String name3 = "tom";

	public void display() {
		String name1 = "rose";
					class Inner2 {
						//String name = "jack";
						public void print() {
							System.out.println("姓名1：" + name1);
							System.out.println("姓名2:"+name1);
							System.out.println("姓名3:"+name3);
//							new Outer2().display();
						}
					}
//		System.out.println(name);
		new Inner2().print();
	}

}

public class Demo02 {
	public static void main(String args[]) {
		Outer2 o = new Outer2();
		o.display();
	}
}
